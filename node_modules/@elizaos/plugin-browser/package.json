{
  "name": "@elizaos/plugin-browser",
  "description": "Plugin for browser actions and web scraping",
  "version": "1.0.3",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "plugin",
    "elizaos",
    "browser",
    "web-scraping"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@elizaos/core": "^1.0.0",
    "@elizaos/plugin-anthropic": "^1.0.0-beta.54",
    "@elizaos/plugin-bootstrap": "^1.0.0-beta.76",
    "@elizaos/plugin-knowledge": "^1.0.0-beta.78",
    "@elizaos/plugin-openai": "^1.0.0-beta.74",
    "@elizaos/plugin-telegram": "^1.0.0-beta.55",
    "@elizaos/plugin-twitter": "^1.0.0-beta.57",
    "@types/uuid": "10.0.0",
    "capsolver-npm": "2.0.2",
    "fluent-ffmpeg": "2.1.3",
    "glob": "11.0.0",
    "patchright": "1.50.1",
    "uuid": "11.0.3",
    "zod": "3.25.23"
  },
  "devDependencies": {
    "tsup": "8.5.0",
    "typescript": "5.8.3",
    "prettier": "3.5.3"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "prettier --write ./src",
    "test": "elizaos test",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src",
    "clean": "rm -rf dist .turbo node_modules .turbo-tsconfig.json tsconfig.tsbuildinfo"
  },
  "peerDependencies": {
    "whatwg-url": "7.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "zod": "3.25.23"
  },
  "gitHead": "646c632924826e2b75c2304a75ee56959fe4a460",
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {
      "CAPSOLVER_API_KEY": {
        "type": "string",
        "description": "API key used by the CaptchaSolver library to solve hCaptcha and reCaptcha challenges.",
        "required": false,
        "sensitive": true
      }
    }
  }
}
