{
    "name": "@elizaos/plugin-analytics",
    "version": "1.0.1",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/elizaos-plugins/plugin-analytics.git"
    },
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "@elizaos/core": "latest",
        "axios": "^1.7.9",
        "typescript": "^5.8.3",
        "zod": "4.1.11"
    },
    "devDependencies": {
        "prettier": "3.5.3",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "1.6.1"
    },
    "scripts": {
        "build": "bun run build.ts",
        "dev": "bun run build.ts --watch",
        "test": "elizaos test",
        "lint": "prettier --write ./src",
        "clean": "rm -rf dist .turbo node_modules .turbo-tsconfig.json tsconfig.tsbuildinfo",
        "format": "prettier --write ./src",
        "format:check": "prettier --check ./src"
    },
    "publishConfig": {
        "access": "public"
    },
    "agentConfig": {
        "pluginType": "elizaos:plugin:1.0.0",
        "pluginParameters": {
            "BIRDEYE_API_KEY": {
                "type": "string",
                "description": "Birdeye API key for Solana data",
                "required": true,
                "sensitive": true
            },
            "COINMARKETCAP_API_KEY": {
                "type": "string",
                "description": "CoinMarketCap API key for multi-chain data",
                "required": false,
                "sensitive": true
            },
            "CODEX_API_KEY": {
                "type": "string",
                "description": "Codex API key for holder and sniper analytics",
                "required": false,
                "sensitive": true
            }
        }
    }
}