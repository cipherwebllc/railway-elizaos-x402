{
    "name": "@elizaos/plugin-pyth-data",
    "version": "1.0.0",
    "description": "Pyth Network data plugin for Eliza",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "tsup",
        "test": "bun test",
        "test:coverage": "bun test --coverage",
        "clean": "rimraf dist",
        "format": "bunx prettier --write \"src/**/*.{ts,js,json}\"",
        "lint": "bunx eslint src --ext .ts,.js",
        "typecheck": "tsc --noEmit --module NodeNext --moduleResolution NodeNext",
        "build:schemas": "openapi-zod-client ./schema.json --output src/types/zodSchemas.ts",
        "pull:schema": "curl -o schema.json -z schema.json https://hermes.pyth.network/docs/openapi.json",
        "prebuild": "bun run pull:schema && bun run build:schemas"
    },
    "dependencies": {
        "@elizaos/core": "latest",
        "@pythnetwork/client": "^2.22.0",
        "@pythnetwork/hermes-client": "^1.3.0",
        "@solana/web3.js": "1.95.8",
        "@zodios/core": "^10.9.6",
        "ajv": "^8.12.0",
        "buffer": "6.0.3",
        "chalk": "^5.4.1",
        "cli-table3": "^0.6.5",
        "cross-fetch": "^4.0.0",
        "eventsource": "^3.0.2",
        "jstat": "^1.9.6",
        "ora": "^8.1.1",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/bun": "latest",
        "@types/jest": "^29.5.5",
        "@types/node": "^20.8.2",
        "@typescript-eslint/eslint-plugin": "^6.7.4",
        "@typescript-eslint/parser": "^6.7.4",
        "eslint": "^8.50.0",
        "openapi-zod-client": "^1.18.1",
        "prettier": "^3.4.2",
        "rimraf": "^5.0.5",
        "tsup": "^8.0.0",
        "typescript": "^5.2.2"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "keywords": [
        "eliza",
        "plugin",
        "pyth",
        "oracle",
        "price-feed"
    ],
    "author": "Eliza Team",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/elizaos/v1-registry.git"
    },
    "bugs": {
        "url": "https://github.com/elizaos/v1-registry/issues"
    },
    "homepage": "https://github.com/elizaos/v1-registry#readme",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "files": [
        "dist"
    ],
    "agentConfig": {
        "pluginType": "elizaos:plugin:1.0.0",
        "pluginParameters": {
            "PYTH_NETWORK_ENV": {
                "type": "string",
                "description": "Environment for Pyth network, can be 'mainnet' or 'testnet'.",
                "required": false,
                "sensitive": false
            },
            "PYTH_MAX_RETRIES": {
                "type": "string",
                "description": "Maximum number of retries for Pyth network requests.",
                "required": false,
                "sensitive": false
            },
            "PYTH_RETRY_DELAY": {
                "type": "string",
                "description": "Delay between retries for Pyth network requests in milliseconds.",
                "required": false,
                "sensitive": false
            },
            "PYTH_LOG_LEVEL": {
                "type": "string",
                "description": "Log level for Pyth network operations, can be 'error', 'warn', 'info', or 'debug'.",
                "required": false,
                "sensitive": false
            },
            "PYTH_MAX_PRICE_STREAMS": {
                "type": "string",
                "description": "Maximum number of price streams for Pyth network.",
                "required": false,
                "sensitive": false
            }
        }
    }
}